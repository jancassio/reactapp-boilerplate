{
  "parserOptions": {
    "sourceType": "module",
  },
  
  "ecmaFeatures": {
    "blockBindings": true,
    "forOf": true,
    "jsx": true,
    "modules": true,
    "arrowFunctions": true,
    "classProperties": true,
    "templateStrings": true,
    "destructuring": true,
    "spread": true
  },

  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },

  "rules": {
    "strict": 0,
    "no-underscore-dangle": 0,
    "quotes": [
      2,
      "single"
    ]
  }
}
